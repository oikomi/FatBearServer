docker run -p 3306:3306 --name mysql -v /root/mysql/conf/my.cnf:/etc/mysql/my.cnf -v /root/mysql/log:/home/mysql/log -v /root/mysql/data:/home/mysql/data -e MYSQL_ROOT_PASSWORD=123456 -d mysql:8.0.34